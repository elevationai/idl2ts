module Example {
    interface Calculator {
        long add(in long a, in long b);
        long subtract(in long a, in long b);
        long multiply(in long a, in long b);
        long divide(in long a, in long b) raises (DivisionByZero);
        readonly attribute long lastResult;
        attribute string description;
    };
    
    struct Result {
        long value;
        string message;
    };
    
    exception DivisionByZero {
        string message;
    };
    
    enum OperationType {
        ADD,
        SUBTRACT,
        MULTIPLY,
        DIVIDE
    };
    
    typedef sequence<long> NumberList;
    
    const long MAX_VALUE = 1000000;
};
{
  "name": "@eai/idl2ts",
  "version": "2.0.0",
  "description": "CORBA IDL to TypeScript compiler for Deno",
  "exports": {
    ".": "./src/index.ts",
    "./cli": "./src/cli.ts"
  },
  "tasks": {
    "test": "deno test --allow-read --allow-write test/"
  },
  "fmt": {
    "indentWidth": 2,
    "semiColons": true,
    "singleQuote": true
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "compilerOptions": {
    "strict": true,
    "lib": ["deno.window"]
  },
  "license": "MIT",
  "repository": "github:elevationai/idl2ts",
  "keywords": [
    "idl",
    "corba",
    "typescript",
    "compiler",
    "code-generation",
    "transpiler"
  ],
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@std/testing": "jsr:@std/testing@^1.0.15"
  }
}
